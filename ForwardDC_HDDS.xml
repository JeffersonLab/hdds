				unit="cm"	/>
    <real	name="rmin"	value="3.50"	comment="inner radius"
				unit="cm"	/>
    <real	name="rmax"	value="60.0"	comment="outer radius"
				unit="cm"	/>
    <real_array	name="phi"	values="0 360"	comment="phi range ()"
				unit="deg"	/>
    <real	name="dphi"	value="6283.18"	comment="angle of wedges"
				unit="mrad"	/>
    <int	name="type"	value="3"	/>
    <string	name="speci"	value="ALL"	comment="SPC or ALL"	   />
    <int	name="nstrip"	value="119"	comment="number of wires"  />
    <real	name="c0_r"	value="0.00"
				unit="cm"	/>
    <real	name="c0_phi"	value="0.00"
				unit="deg"	/>
    <real	name="pitch"	value="1.0"	comment="wire spacing"
				unit="cm"	/>
    <real	name="stereo"	value="0.00"	comment="phi orientation"
				unit="deg"	/>
    <real	name="eff_hit"	value="0.96"	comment="hit efficiency"
				unit="none"	/>
    <real	name="siga"	value="0.015"	comment="hit resolution"
				unit="cm"	/>
    <real	name="sigb"	value="0.00"	comment="resolution par b"
				unit="cm"	/>
    <real	name="sigc"	value="0.00"	comment="resolution par c"
				unit="cm"	/>
  </parameters>

