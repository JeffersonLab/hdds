<section name        = "ForwardDC"
         version     = "1.0"
         date        = "2001-04-15"
         author      = "R.T. Jones"
         top_volume  = "FDC"
         specification = "v1.0">

<!-- Origin of ForwardDC is center of upstream surface
     of the most upstream chamber.                        -->

  <composition name="ForwardDC">
    <posXYZ volume="forwardDC" X_Y_Z="0.0  0.0  88.0" />
  </composition>

  <composition name="forwardDC" envelope="FDC">
    <posXYZ volume="forwardDC_unit" X_Y_Z="0.0  0.0  -85.0">
      <module value="1" />
    </posXYZ>
    <posXYZ volume="forwardDC_unit" X_Y_Z="0.0  0.0  -79.0">
      <module value="2" />
    </posXYZ>
    <posXYZ volume="forwardDC_unit" X_Y_Z="0.0  0.0  -33.0">
      <module value="3" />
    </posXYZ>
    <posXYZ volume="forwardDC_unit" X_Y_Z="0.0  0.0  -27.0">
      <module value="4" />
    </posXYZ>
    <posXYZ volume="forwardDC_unit" X_Y_Z="0.0  0.0   23.0">
      <module value="5" />
    </posXYZ>
    <posXYZ volume="forwardDC_unit" X_Y_Z="0.0  0.0   29.0">
      <module value="6" />
    </posXYZ>
    <posXYZ volume="forwardDC_unit" X_Y_Z="0.0  0.0   79.0">
      <module value="7" />
    </posXYZ>
    <posXYZ volume="forwardDC_unit" X_Y_Z="0.0  0.0   85.0">
      <module value="8" />
    </posXYZ>
  </composition>

  <composition name="forwardDC_unit" envelope="FDCH">
    <posXYZ volume="forwardDC_module" X_Y_Z="0.0 0.0 -2.0" rot="0   0    0">
      <layer value="1" />
    </posXYZ>
    <posXYZ volume="forwardDC_module" X_Y_Z="0.0 0.0  0.0" rot="0   0   60">
      <layer value="2" />
    </posXYZ>
    <posXYZ volume="forwardDC_module" X_Y_Z="0.0 0.0  2.0" rot="0   0  -60">
      <layer value="3" />
    </posXYZ>
  </composition>

  <composition name="forwardDC_module" envelope="FDMO">
    <posXYZ volume="FDCA" X_Y_Z="0.0 0.0 0.0">
      <plane value="2" />
    </posXYZ>
    <posXYZ volume="FDCC" X_Y_Z="0.0 0.0 -0.996" rot="0   0   60">
      <plane value="1" />
    </posXYZ>
    <posXYZ volume="FDCC" X_Y_Z="0.0 0.0 +0.996" rot="0   0  -60">
      <plane value="3" />
    </posXYZ>
  </composition>

  <tubs name="FDC" Rio_Z="3.5 62.0 176.0" material="Air"
                                          comment="forward drift chambers" />

  <tubs name="FDCH" Rio_Z=" 3.5 61.0 6.0" material="Air"        />
  <tubs name="FDMO" Rio_Z=" 3.5 61.0 2.0" material="Air"        />
  <tubs name="FDCA" Rio_Z=" 3.5 60.0 1.984" material="chamberGas"
					    sensitive="true" 	/>
  <tubs name="FDCC" Rio_Z=" 3.5 60.0 0.008" material="Mylar"
					    sensitive="true" 	/>
  <tubs name="FDCS" Rio_Z="60.0 61.0 6.0" material="Aluminum"   />

  <parameters name="forwardDC_pars" type="mcfast">
    <int	name="nlyr"	value="9"	comment="number of layers" />
  </parameters>

  <parameters name="forwardDCanode_pars" type="mcfast">
    <reference	name="mat"	value="chamberGas" />
    <int	name="nwed"	value="1"	comment="number of wedges" />
