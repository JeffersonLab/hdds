</composition>

<!-- PMT column 1..6-->
<composition name="PMTgrid">
  <mposZ volume="PMTrow" ncopy="6" X_Y="0.0 0.0" Z0="-13.25" dZ="5.3" >
    <column value="1" step="1"/>
